<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://cardif.bnpparibas.com/lam/schema/thirdparty/address/v1"
	xmlns:tns="http://cardif.bnpparibas.com/lam/schema/thirdparty/address/v1"
	elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xs:element name="Address" type="tns:Address_Type"></xs:element>

	<xs:complexType name="Address_Type">
		<xs:sequence>
			<xs:element name="townName" type="xs:string" minOccurs="0">
			</xs:element>
			<xs:element name="fullAddressName" type="xs:string" minOccurs="0">
			</xs:element>
		</xs:sequence>
	</xs:complexType>


</xs:schema>